- if (!everyauth.loggedIn)
  h2 Not Authenticated
  #github-login
    a.github(href='/auth/github')
      span#github_login_text Login with GitHub
- else
  h2 Authenticated
  - if (everyauth.github)
    h3 GitHub User Data
    p= JSON.stringify(everyauth.github.user)
  h3
    a(href='/logout') Logout
